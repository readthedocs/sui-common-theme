{
  "name": "@readthedocs/sui-common-theme",
  "version": "1.0.2",
  "description": "Read the Docs SUI common theme",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --check '**/*.overrides' '**/*.variables' '*.json'",
    "format": "prettier --write '**/*.overrides' '**/*.variables' '*.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/readthedocs/sui-common-theme.git"
  },
  "author": "Read the Docs, Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/readthedocs/sui-common-theme/issues"
  },
  "homepage": "https://github.com/readthedocs/sui-common-theme#readme",
  "dependencies": {
    "fomantic-ui-less": "^2.9.4"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "**/*.overrides",
          "**/*.variables"
        ],
        "options": {
          "parser": "less"
        }
      }
    ]
  }
}
